{
	"manifest_version": 2,

	"name": "GET XPATH",
	"description": "GET XPATH",
	"version": "1.0",

	
  "web_accessible_resources": ["css/*", "fonts/*", "js/lib/*", "js/languages/*"],
  "permissions": [
    "tabs",
    "webRequest",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*"
            ],
        "js": ["myscript.js"]
        
        }
    ] 
}
